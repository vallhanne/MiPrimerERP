<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.example.PlaylistTrackClass" table="PlaylistTrack" schema="Chinook">
        <composite-id mapped="true" class="org.example.PlaylistTrackClassPK">
            <key-property name="playlistId" column="PlaylistId"/>
            <key-property name="trackId" column="TrackId"/>
        </composite-id>
    </class>
</hibernate-mapping>